# ╔═╡ f8da3c17-0feb-4957-bc50-8c496b135af5
dataFrameNY = CSV.File(Downloads.download(url)) |> DataFrame

# ╔═╡ 9bffba9e-c708-4449-b15b-2ccf905008c8
names(dataFrameNY)

# ╔═╡ eaf21cfb-71c9-4683-afd6-eb0aa46f1fb3
first(dataFrameNY, 5)

# ╔═╡ 70eb0c27-0b90-49ee-89af-d6621402ff22
dataFrameNY.fare_amount |> mean

